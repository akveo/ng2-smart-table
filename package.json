    "gulp-htmlmin": "3.0.0",
    "gulp-if": "2.0.2",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.4.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
