    "@angular/compiler-cli": "4.0.0",
    "@angular/platform-server": "4.0.0",
    "@types/chalk": "0.4.31",
    "@types/gulp": "4.0.2",
    "@types/highlight.js": "9.1.9",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.58",
    "@types/node": "6.0.63",
